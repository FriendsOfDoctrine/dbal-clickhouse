{
    "name": "imdad/dbal-clickhouse",
    "type": "library",
    "description": "Doctrine DBAL driver for ClickHouse with some fixes for MySQL data types",
    "keywords": [
        "doctrine",
        "dbal",
        "clickhouse",
        "driver"
    ],
    "homepage": "http://github.com/imdad/dbal-clickhouse",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochalygin Aleksandr",
            "email": "a@mochalygin.ru"
        },
        {
            "name": "Mitrofanov Nikolay",
            "email": "mitrofanovnk@yandex.ru"
        },
        {
            "name": "SK Imdad"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-PDO": "*",
        "ext-pcre": "*",
        "doctrine/dbal": "^2.7",
        "smi2/phpClickHouse": "^1.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FOD\\DBALClickHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOD\\DBALClickHouse\\Tests\\": "tests/"
        }
    }
}
